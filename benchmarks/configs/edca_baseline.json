{
    "openml_splits": false,
    "task": "classification",
    "dataset": [
        "adult.csv",
        "credit-card.csv",
        "portuguese-bank-marketing.csv"
    ],
    "save_path": "30-min/fairness-experiments/edca-mcc-recall-baseline",
    "kfold": 5,
    "run-fold": null,
    "run_all_seeds": true,
    "seed_pos": 0,
    "n_iterations": 150,
    "time_budget": 1800,
    "n_jobs": 5,
    "metric": "mcc_recall",
    "validation_size": 0.25,
    "train_edca": true,
    "train_flaml": false,
    "flaml_hpo_method": "cfo",
    "train_tpot": false,
    "tpot_template": "Selector-Transformer-Classifier",

    "verbose": 10,
    "population": 25,
    "prob_mutations": 0.3,
    "prob_mutation_model": 0.5,
    "prob_crossover": 0.8,
    "tournament_size": 3,
    "elitism_size": 1,
    "automatic_data_optimization": true,
    "binary_sampling_component": false,
    "sampling": true,
    "feature_selection": true,
    "data_augmentation": false,
    "fitness_params" : {
        "metric": 1.0,
        "data_size": 0,
        "training_time": 0,
        "balance_metric": 0,
        "fairness_metric": 0
    },
    
    "class_balance_mutation": true,
    "mutation_factor": 0.5,
    "uniform_crossover": false,
    "patience": 5,
    "early_stop": null,
    "sampling_start": 0,
    "mutation_size_neighborhood": 10,
    "mutation_percentage_change": 0.1,
    "flaml_ms": false,
    
    "fairness_params" : {
        "adult.csv" : {
            "sensitive_attributes" : ["age", "race", "sex"],
            "demographic_parity": 0,
            "equal_opportunity": 0,
            "equalized_odds": 0,
            "abroca" : 0,
            "positive_class" : ">50K",
            "bin_class": {
                "age" : [25, 60]
            }
        },
        "portuguese-bank-marketing.csv" : {
            "sensitive_attributes" : ["age", "marital"],
            "demographic_parity": 0,
            "equal_opportunity": 0,
            "equalized_odds": 0,
            "abroca" : 0,
            "positive_class" : "yes",
            "bin_class": {
                "age" : [25, 60]
            }
        },
        "credit-card.csv" : {
            "sensitive_attributes" : ["x2", "x4", "x3"],
            "demographic_parity": 0,
            "equal_opportunity": 0,
            "equalized_odds": 0,
            "abroca" : 0,
            "positive_class" : 1,
            "bin_class" : null
        },
        "diabetes-hospital-uci.csv" : {
            "sensitive_attributes" : ["gender", "age", "race"],
            "demographic_parity": 0.333,
            "equal_opportunity": 0,
            "equalized_odds": 0.333,
            "abroca" : 0.333,
            "positive_class" : true,
            "bin_class": null
        }
    }
}